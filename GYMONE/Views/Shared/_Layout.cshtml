<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title -Caritas LTE</title>
     <link href="~/Images/hand.png"  rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />


    @Scripts.Render("~/bundles/modernizr")
        @Styles.Render("~/Content/css")
</head>
<body>
    <header>
      
            <div class="col-md-12 col-sm-12">
                <section id="login">
                </section>
            </div>
        
    </header>
    <div id="body">
        @RenderSection("featured", required: false)
        <section class="">
            @RenderBody()
        </section>
    </div>
  <footer>
        <div class="container">
            <div class="text-center footer">
                Developed By <a href="mailto:Charlesobika@yahoo.com">Charles Obika</a>
                Designed By <a href="mailto:Chijiokeobika@gmail.com">Caritas-LTE</a>
            </div>
        </div>
    </footer>
  
    
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/themes/base/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)
</body>
</html>
